# Modules with fixed Implementation
WCAML_LIB= config property event

# Modules with fixed API
WCAML_API= main user widget window control

WCAML_MLI=$(addsuffix .mli,$(WCAML_LIB) $(WCAML_API))
WCAML_ML=$(addsuffix .ml,$(WCAML_LIB))

